        <Label Content="   IES" HorizontalAlignment="Left" Margin="548,79,0,0" VerticalAlignment="Top" Background="#FF497FCD" FontSize="10" Width="39"/>
        <Label Name="GPIO" Content="000" HorizontalAlignment="Left" Margin="58,113,0,0" VerticalAlignment="Top" Height="27" Width="40"/>
        <Label Name="VALUE" Content="000000000" HorizontalAlignment="Left" Margin="103,113,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.774,0.611" Width="74" Height="27"/>
        <ComboBox x:Name="MODE" HorizontalAlignment="Left" Margin="182,112,0,0" VerticalAlignment="Top" Width="41" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="MODE_SelectionChanged"/>
        <ComboBox x:Name="DIR" HorizontalAlignment="Left" Margin="228,112,0,0" VerticalAlignment="Top" Width="59" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="DIR_SelectionChanged2"/>
        <ComboBox x:Name="DOUT" HorizontalAlignment="Left" Margin="292,112,0,0" VerticalAlignment="Top" Width="77" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="DOUT_SelectionChanged3"/>

        <ComboBox x:Name="DIN" HorizontalAlignment="Left" Margin="374,112,0,0" VerticalAlignment="Top" Width="73" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="DIN_SelectionChanged4"/>
        <ComboBox x:Name="IES" HorizontalAlignment="Left" Margin="548,112,0,0" VerticalAlignment="Top" Width="39" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="IES_SelectionChanged7"/>
        <ComboBox x:Name="PULLSEL" HorizontalAlignment="Left" Margin="671,112,0,0" VerticalAlignment="Top" Width="75" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="PULLSEL_SelectionChanged9"/>
        <ComboBox x:Name="PULLEN" HorizontalAlignment="Left" Margin="592,112,0,0" VerticalAlignment="Top" Width="73" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="PULLEN_SelectionChanged8"/>
        <Label Content="SMT" HorizontalAlignment="Left" Margin="503,79,0,0" VerticalAlignment="Top" Background="#FF497FCD" FontSize="10" Width="40"/>
        <Label Content=" DRIVE" HorizontalAlignment="Left" Margin="452,79,0,0" VerticalAlignment="Top" Background="#FF497FCD" FontSize="10" Width="46" RenderTransformOrigin="2.441,0.525"/>
        <ComboBox x:Name="DRIVE" HorizontalAlignment="Left" Margin="452,112,0,0" VerticalAlignment="Top" Width="46" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="DRIVE_SelectionChanged5"/>
        <ComboBox x:Name="SMT" HorizontalAlignment="Left" Margin="503,112,0,0" VerticalAlignment="Top" Width="40" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="SMT_SelectionChanged6"/>
        <Button Content="set" HorizontalAlignment="Left" Margin="24,115,0,0" VerticalAlignment="Top" Width="29" Background="#FF28BF36" Click="Button_Click_Set_Gpio_Value"/>
        <Ellipse Name="led" Fill="#FFDC1111" HorizontalAlignment="Left" Height="10" Margin="764,18,0,0" Stroke="Red" VerticalAlignment="Top" Width="10" RenderTransformOrigin="-0.682,-3"/>
        <Button Content="PMIC" HorizontalAlignment="Left" Margin="305,10,0,0" VerticalAlignment="Top" Width="43" RenderTransformOrigin="-0.432,-0.643" Height="30" Click="Button_Click_Get_PMIC_Info"/>
        <Button Content="内存" HorizontalAlignment="Left" Margin="353,10,0,0" VerticalAlignment="Top" Width="43" RenderTransformOrigin="-0.432,-0.643" Height="30" Click="Button_Click_Get_Mem_Info"/>
        <Button Content="电池" HorizontalAlignment="Left" Margin="401,10,0,0" VerticalAlignment="Top" Width="43" RenderTransformOrigin="-0.432,-0.643" Height="30" Click="Button_Click_Get_Battery_Info"/>
        <Button Content="提取log" HorizontalAlignment="Left" Margin="576,10,0,0" VerticalAlignment="Top" Width="54" RenderTransformOrigin="-0.432,-0.643" Height="30" Click="Button_Click_Get_MTK_Log"/>
        <Button Content="截屏" HorizontalAlignment="Left" Margin="449,10,0,0" VerticalAlignment="Top" Width="63" RenderTransformOrigin="-0.432,-0.643" Height="30" Click="Button_Click_ScreenCap"/>
        <Button Content="reboot" HorizontalAlignment="Left" Margin="517,10,0,0" VerticalAlignment="Top" Width="54" RenderTransformOrigin="-0.432,-0.643" Height="30" Click="Button_Click_Reboot"/>
        <Label Content="  R1 R0" Name="R1R0" HorizontalAlignment="Left" Margin="751,79,0,0" VerticalAlignment="Top" Background="#FF497FCD" FontSize="10" Width="45" Visibility="Collapsed"/>
        <ComboBox x:Name="R1R0_Value" HorizontalAlignment="Left" Margin="751,112,0,0" VerticalAlignment="Top" Width="45" Height="24" RenderTransformOrigin="0.497,-0.226" SelectionChanged="R0R1_SelectionChanged10" Visibility="Collapsed"/>
    </Grid>

</Window>
